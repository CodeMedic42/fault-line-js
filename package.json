  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.16.1",
